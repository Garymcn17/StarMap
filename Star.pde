class Star
{
  int hab;
  String name;
  float distance;
  float Xg;
  float Yg;
  float Zg;
  float AbsMag; // Size of star
  
  
  Star(int hab, String name, float distance, float Xg, float Yg, float Zg)
  {
    this.hab = hab;
    this.name = name;
    this.distance = distance;
    this.Xg = Xg;
    this.Yg = Yg;
    this.Zg = Zg;
    this.AbsMag = AbsMag;
  }
  
}